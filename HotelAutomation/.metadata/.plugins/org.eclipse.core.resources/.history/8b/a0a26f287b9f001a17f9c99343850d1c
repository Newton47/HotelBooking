package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class loginPage {
	
	WebDriver driver;
	
	@FindBy(name="userName")
	WebElement username;
	
	@FindBy(name="userPwd")
	WebElement password;
	
	@FindBy(className="btn")
	WebElement LoginBtn;
	
	@FindBy(id="userErrMsg")
	
	

}
